}
layer {
  name: "reshape"
  type: "Reshape"
  bottom: "target"
  top: "target2"
  reshape_param {
    shape {
        dim: 0  # copy the dimension from below
        dim: -1 # infer it from the other dimensions
    }
  }
}
layer {
  name: "concate"
  type: "Concat"
  bottom: "target2"
  bottom: "normalized_redundancy"
  top: "redundancy_coords2"
}
